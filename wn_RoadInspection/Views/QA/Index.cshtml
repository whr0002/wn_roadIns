@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    ViewBag.Title = "Quality Control";
}

<h2>Quality Control</h2>

<div class="row">
    <div class="col-md-12">
        @Html.ActionLink("BACK", "Index", "SuperAdmin")

    </div>

    <div class="col-md-12">
        <ul>
            @foreach (var role in Model)
            {
                <li>
                    @Html.ActionLink(@role.Name, "ViewContainer", new { roleName = @role.Name })
                </li>
            }
            <li>
                @Html.ActionLink("unknown", "ViewContainer", new { roleName = "unknown" })
            </li>
        </ul>
    </div>
</div>